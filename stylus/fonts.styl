@font-face
	font-family 'digital'
	src url(../fonts/'DIGITALDREAM-webfont.eot')
	src url(../fonts/'DIGITALDREAM-webfont.eot?#iefix') format('embedded-opentype'),
		url(../fonts/'DIGITALDREAM-webfont.woff2') format('woff2'),
		url(../fonts/'DIGITALDREAM-webfont.woff') format('woff'),
		url(../fonts/'DIGITALDREAM-webfont.ttf') format('truetype'),
		url(../fonts/'DIGITALDREAM-webfont.svg#digital_dream_fat_narrowRg') format('svg')
	font-weight normal
	font-style normal

@font-face
	font-family 'icomoon'
	src url('../fonts/icono/icomoon.eot?m5taku')
	src url('../fonts/icono/icomoon.eot?#iefixm5taku') format('embedded-opentype'),
		url('../fonts/icono/icomoon.woff?m5taku') format('woff'),
		url('../fonts/icono/icomoon.ttf?m5taku') format('truetype'),
		url('../fonts/icono/icomoon.svg?m5taku#icomoon') format('svg')
	font-weight normal
	font-style normal

[class^="icon-"], [class*=" icon-"]
	color #00FFFF
	font-family 'icomoon'
	speak none
	font-style normal
	font-size 4em
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale
	animation 2s icon infinite
	
@keyframes icon {
     0% {
		opacity: .4;		
		}
	 50% {
		opacity: .7;		
		}   
    100% {
		opacity: .4;		
		}
}

